{
    "version": 2,
    "builds": [
        {
            "src": "dist/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/main.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "functions": {
        "dist/main.js": {
            "maxDuration": 30
        }
    },
    "outputDirectory": "dist",
    "installCommand": "npm ci",
    "buildCommand": "npm run build",
    "devCommand": "npm run start:dev"
}
